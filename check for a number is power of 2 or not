import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.print("Enter the digit to check: ");
		int num=sc.nextInt();
		if(((num)&(num-1))==0){
		    System.out.print("True");
		}
		else{
		    System.out.print("False");
		}
	}
}
