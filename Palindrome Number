class Solution {
    public boolean isPalindrome(int x) {
      int val=x;
      int cont=0;
      int pal=0;
      int rem=0;
      if(val>0){
      while(val!=0){ 
          rem=val%10;
          pal=pal*10+rem;
          val=val/10; 
      } 
      }
      if(pal==x){
          return true ;
      }
      return false;
    }
}
